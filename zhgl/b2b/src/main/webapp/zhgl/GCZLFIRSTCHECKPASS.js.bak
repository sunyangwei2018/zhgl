GCZLFIRSTCHECKPASS.define({
	 "setButton": function(iState){
		  //新增
		  if((iState==0)||(iState==2)){
			  GCZLFIRSTCHECKPASS.getTab().find("#add").linkbutton('enable');
		  }else{
			  GCZLFIRSTCHECKPASS.getTab().find("#add").linkbutton('disable');
		  }
		  
		  //保存
		  if(iState==1){
			  GCZLFIRSTCHECKPASS.getTab().find("#save").linkbutton('enable');;
		  }else{
			  GCZLFIRSTCHECKPASS.getTab().find("#save").linkbutton('disable');
		  }
		  
		  //修改
		  if(iState==2){
				GCZLFIRSTCHECKPASS.getTab().find("#edit").linkbutton('enable');
				GCZLFIRSTCHECKPASS.getTab().find("#del").linkbutton('enable');
		  }else{
				GCZLFIRSTCHECKPASS.getTab().find("#edit").linkbutton('disable');
				GCZLFIRSTCHECKPASS.getTab().find("#del").linkbutton('disable');
		  }
		  
		  //取消
		  if(iState!=0){
			  GCZLFIRSTCHECKPASS.getTab().find("#cancel").linkbutton('enable');
		  }else{
			  GCZLFIRSTCHECKPASS.getTab().find("#cancel").linkbutton('disable');
		  }
	},
	"add": function(){
		this.setButton(1);
		GCZLFIRSTCHECKPASS.getTab().find("#billno").textbox("readonly",false);
		this.definefieldData();
	},
	"del": function(){
		
	},
	"clearData": function(){
		GCZLFIRSTCHECKPASS.setData({});
	},
	"save": function(){
		var lenght = GCZLFIRSTCHECKPASS.getTab().find(".bhbj:checked").length;
		if(!ui.isNull(GCZLFIRSTCHECKPASS.getData().taskId)){//审核
			var variables = {};
			variables["msg"] = GCZLFIRSTCHECKPASS.getTab().find("#msg").combobox("getValue");
			variables["digestCust"] = GCZLFIRSTCHECKPASS.getTab().find("#digestCust").textbox("getValue");
			var	XmlData =[];
			var queryField = {};
				queryField["userId"] = userInfo.USERID;
				queryField["taskId"] = GCZLFIRSTCHECKPASS.getData().taskId;
				queryField["taskName"] = GCZLFIRSTCHECKPASS.getData().taskName;
				queryField["variables"] = variables;
			//XmlData[0]["nownet"] = ui.isNull(userInfo.WD01)?"":userInfo.WD01;
			var ajaxJson = {};
			
			if(variables["msg"]=="驳回"){
				GCZLFIRSTCHECKPASS.getTab().find("#digestCust").textbox("enableValidation");
				var bhData = [];
				if(lenght==0){
					ui.tip("请至少选择一个资料驳回")
					return false;
				}
				//验证必填项
				if(!GCZLFIRSTCHECKPASS.getTab().find("#GCZLFIRSTCHECKPASS").form('validate')){
					return false;
				}
				var bhData = [];
				//封盒驳回数据
				$.each(GCZLFIRSTCHECKPASS.getTab().find(".bhbj:checked"),function(index,val){
					var bhfile={};
					bhfile["GCZLID"] = GCZLFIRSTCHECKPASS.getData().bizKey;
					bhfile["TYPE"] = $(val).val();
					bhData.push(bhfile);
				});
				queryField["returnFiles"] = bhData;
				ajaxJson["src"] = "gczl/updateGczlReturn.do";
				ajaxJson["data"] = {"json":JSON.stringify(queryField)};
				var resultData = ui.ajax(ajaxJson).data;
				if(resultData.respCode=="000000"){
					ui.tip(resultData.respMsg);
				}
			}else{
				GCZLFIRSTCHECKPASS.getTab().find("#digestCust").textbox("disableValidation");
				ajaxJson["src"] = "gczl/auditApprove.do";
				ajaxJson["data"] = {"json":JSON.stringify(queryField)};
				var resultData = ui.ajax(ajaxJson).data;
				if(resultData.respCode=="000000"){
					ui.tip(resultData.respMsg);
				}
			}
			if($.type(GCZLFIRSTCHECKPASS.getData().query)=="function"){
				GCZLFIRSTCHECKPASS.getData().query();
			}	
			ui.closeSelectedTab();
		}
	},
