		var variables = {};
		variables["msg"] = GCZLFIRSTCHECKPASS.getTab().find("#msg").combobox("getValue");
		variables["digestCust"] = GCZLFIRSTCHECKPASS.getTab().find("#digestCust").textbox("getValue");
