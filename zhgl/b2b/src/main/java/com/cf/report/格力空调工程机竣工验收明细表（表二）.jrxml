	<parameter name="GCZLID" class="java.lang.String"/>
	<queryString>
		<![CDATA[select a.gc_id,
       b.itemid ,
       b.gczlid ,
       b.spwj_tm ,
       b.spwj_id ,
       b.spwj_name ,
       b.spnj_tm,
       b.spnj_id,
       b.spnj_name ,
       b.customer_lxr ,
       b.customer_tel ,
       b.customer_address,
       rownum,
      (case
           when rownum<101 then 100
           when rownum>100 and rownum <201  then 200
           when rownum>200 and rownum <301  then 300
           when rownum>300 and rownum <401  then 400
           when rownum>400 and rownum <501  then 500
       else 1000
       end
      )GC_TYPE
from gczl a,gczlitem b
where a.gczlid = b.gczlid and a.gczlid =  $P{GCZLID}
 order by b.customer_address, b.spnj_name]]>
